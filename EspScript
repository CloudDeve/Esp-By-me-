local a='!esp'local b='!unesp'local c='!locate 'local d='!unlocate 'local e=game.Players.LocalPlayer;local f=true;local g=true;local h=false;local i=function()for j,k in pairs(game:GetService('Players'):GetPlayers())do if k~=e then if not k.Character:FindFirstChildOfClass('Highlight')then local l=Instance.new('Highlight')l.Parent=k.Character;l.Adornee=k.Character;l.Name=k.Name;local m=Instance.new('BillboardGui')m.Parent=k.Character;m.Adornee=k.Character;m.StudsOffset=Vector3.new(0,4,0)m.Size=UDim2.new(0,200,0,50)m.AlwaysOnTop=true;local n=Instance.new('TextLabel')n.Parent=m;n.Size=UDim2.new(0,200,0,50)local o=coroutine.create(function()while f==false do wait(0.3)n.Text='Name:'..k.Name..'|'..'Health:'..math.floor(k.Character.Humanoid.Health)end end)coroutine.resume(o)n.TextScaled=true;n.TextColor3=Color3.fromRGB(255,255,255)n.BackgroundTransparency=1 end end end end;e.Chatted:Connect(function(p)if p:sub(1,a:len()):lower()==a:lower()then f=false;h=true;while f==false do wait(0.3)local q=e.Character or e.CharacterAdded:Wait()i(q)end elseif p:sub(1,b:len()):lower()==b:lower()then f=true;wait(0.3)for q,l in pairs(game:GetService('Players'):GetPlayers())do if l.Character:FindFirstChildOfClass('Highlight')then l.Character:FindFirstChildOfClass('Highlight'):Destroy()l.Character:FindFirstChildOfClass('BillboardGui'):Destroy()end end elseif p:sub(1,c:len()):lower()==c:lower()then g=false;h=true;local r=p:sub(9,p:len())for s,k in pairs(game:GetService('Players'):GetPlayers())do if k.Name==r then if k~=e then if not k.Character:FindFirstChildOfClass('Highlight')then local l=Instance.new('Highlight')l.Parent=k.Character;l.Adornee=k.Character;l.Name=k.Name;local m=Instance.new('BillboardGui')m.Parent=k.Character;m.Adornee=k.Character;m.StudsOffset=Vector3.new(0,4,0)m.Size=UDim2.new(0,200,0,50)m.AlwaysOnTop=true;local n=Instance.new('TextLabel')n.Parent=m;n.Size=UDim2.new(0,200,0,50)local o=coroutine.create(function()while g==false do wait(0.3)n.Text='Name:'..k.Name..'|'..'Health:'..math.floor(k.Character.Humanoid.Health)end end)coroutine.resume(o)n.TextScaled=true;n.TextColor3=Color3.fromRGB(255,255,255)n.BackgroundTransparency=1 end end end end elseif p:sub(1,d:len()):lower()==d:lower()then g=true;h=false;local r=p:sub(11,p:len())for j,k in pairs(game:GetService('Players'):GetPlayers())do if k.Name==r then if k.Character:FindFirstChildOfClass('Highlight')then k.Character:FindFirstChildOfClass('Highlight'):Destroy()k.Character:FindFirstChildOfClass('BillboardGui'):Destroy()if f==false then f=true;while f==true and h==false do wait(0.3)for s,t in pairs(game:GetService('Players'):GetPlayers())do if t.Character:FindFirstChildOfClass('Highlight')then t.Character:FindFirstChildOfClass('Highlight'):Destroy()t.Character:FindFirstChildOfClass('BillboardGui'):Destroy()end end end end end end end end end)game:GetService('Players').PlayerAdded:Connect(function(u)if f==false then local v=u.Character or u.CharacterAdded:Wait()i(v)end end)
